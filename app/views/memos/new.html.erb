<%= form_with(model: @memo, local: true) do |f| %>
<%= render 'shared/error_messages', object: f.object %>


  <div class="form-group">
    <%=f.label :タイトル %>
    <%=f.text_field :title, class:"form-control", id:"email"%>
  </div>
  
  <div class="form-group">
    <%=f.label :重量 %>
    <%=f.text_field :weight, class:"form-control", id:"email"%>
  </div>

  <div class="form-group">
    <%=f.label :回数 %>
    <%=f.text_field :times, class:"form-control", id:"pwd"%>
  </div>

  <div class="form-group">
    <%=f.label :セット数 %>
    <%=f.text_field :set, class:"form-control", id:"pwd"%>
  </div>
  
  <div class="form-group">
    <%=f.label :メモ %>
    <%=f.text_field :memo, class:"form-control", id:"pwd"%>
  </div>

  <%= f.submit"保存", class: "btn btn-primary btn-lg mt-1 mb-3 mr-5 " %>
  

<% end %>

<p class="wenitest">ウェニ投稿</p>